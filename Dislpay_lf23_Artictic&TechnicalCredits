rule "Primo VE - Artistic & Technical Credits 508"
	when
		MARC."508" has any "a" AND NOT
        	(MARC."880" has any "a" AND
        	MARC."880"."6" match "508-.*")
	then
		create pnx."display"."lds23" with MARC."508" subfields "a"
end

rule "Primo VE - Artistic & Technical Credits 880-508"
	when
                MARC is "508" AND	
		MARC."880" has any "a" AND
		MARC."880"."6" match "508-.*"
	then
		create pnx."display"."lds23" with MARC."508" subfields "a"
end
